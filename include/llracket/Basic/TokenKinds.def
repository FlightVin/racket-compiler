#ifndef TOK
#define TOK(ID)
#endif
#ifndef PUNCTUATOR
#define PUNCTUATOR(ID, SP) TOK(ID)
#endif
#ifndef KEYWORD
#define KEYWORD(ID, FLAG) TOK(kw_##ID)
#endif

TOK(unknown)
TOK(eof)
TOK(integer_literal)
TOK(identifier)
TOK(boolean_literal)

PUNCTUATOR(l_paren, "(")
PUNCTUATOR(r_paren, ")")
PUNCTUATOR(l_square, "[")
PUNCTUATOR(r_square, "]")
PUNCTUATOR(plus, "+")
PUNCTUATOR(minus, "-")
PUNCTUATOR(read, "read")
PUNCTUATOR(eq, "eq?")
PUNCTUATOR(lt, "<")
PUNCTUATOR(le, "<=")
PUNCTUATOR(gt, ">")
PUNCTUATOR(ge, ">=")
PUNCTUATOR(and_, "and")
PUNCTUATOR(or_, "or")
PUNCTUATOR(not_, "not")
PUNCTUATOR(setb, "set!")
PUNCTUATOR(vector, "vector")
PUNCTUATOR(vector_length, "vector-length")
PUNCTUATOR(vector_ref, "vector-ref")
PUNCTUATOR(vector_setb, "vector-set!")
PUNCTUATOR(colon, ":")                 // <<< ADDED
PUNCTUATOR(arrow, "->")               // Added previously, ensure it stays if used, currently unused for ->

// Keywords with feature flags
KEYWORD(let, KEYALL)
KEYWORD(if, KEYALL)
KEYWORD(true, KEYALL)
KEYWORD(false, KEYALL)
KEYWORD(while, KEYALL)
KEYWORD(begin, KEYALL)
KEYWORD(void, KEYALL)
KEYWORD(define, KEYALL)              // <<< ADDED

#undef KEYWORD
#undef PUNCTUATOR
#undef TOK